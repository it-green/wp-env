{
  "name": "wp-env",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "wp-env:start": "wp-env start && npm run theme:update",
    "wp-env:update": "wp-env start --update && theme:activate",
    "wp-env:stop": "wp-env stop",
    "wp-env:destroy": "wp-env destroy",
    "theme:update": "wp-env run cli wp theme activate my-theme",
    "sql:export": "wp-env run cli wp db export sql/wpenv.sql",
    "sql:import": "wp-env run cli wp db import sql/wpenv.sql",
    "dev": "run-s wp-env:start && webpack --mode development -w",
    "build": "webpack --mode development && node ./build.js",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wordpress/env": "^9.8.0",
    "autoprefixer": "^10.4.19",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "mini-css-extract-plugin": "^2.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^14.2.1",
    "stylelint": "^16.4.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.0",
    "sweet-scroll": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-fix-style-only-entries": "^0.6.1"
  },
  "dependencies": {
    "browser-sync": "^3.0.2",
    "browser-sync-webpack-plugin": "^2.3.0",
    "fs-extra": "^11.2.0",
    "sass": "^1.75.0",
    "webpack-watched-glob-entries-plugin": "^3.0.0"
  }
}
